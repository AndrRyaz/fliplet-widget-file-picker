<div class="col-xs-4 col-sm-3 col-md-2 item-holder file image document {{#if selected}} selected {{/if}}" data-file-id="{{id}}" data-file-ext="{{ext}}">
    <div class="image-holder">
        <div class="image-overlay" style="opacity: 1">
            <i class="fa fa-file" aria-hidden="true"></i>
        </div>
    </div>
    <div class="image-title">{{name}}</div>
</div>
